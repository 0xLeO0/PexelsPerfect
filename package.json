{
    "name": "pexels-perfect",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "lodash.debounce": "^4.0.8",
        "lodash.throttle": "^4.1.1",
        "pexels": "^1.0.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-scripts": "4.0.1",
        "web-vitals": "^0.2.4"
    },
    "scripts": {
        "start": "npm run watch:css && react-scripts start",
        "build": "npm run build:css && GENERATE_SOURCEMAP=false react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "watch:css": "postcss src/css/tailwind.css -o src/css/style.css",
        "build:css": "postcss src/css/tailwind.css -o src/css/style.css"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "autoprefixer": "^10.0.3",
        "cssnano": "^4.1.10",
        "postcss": "^8.1.10",
        "postcss-cli": "^8.3.0",
        "tailwindcss": "^2.0.1"
    }
}
